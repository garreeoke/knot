{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "labels": {
      "app": "applariat-http-backend"
    },
    "name": "applariat-http-backend",
    "namespace": "kube-system"
  },
  "spec": {
    "ports": [
      {
        "port": 80,
        "protocol": "TCP",
        "targetPort": 8080
      }
    ],
    "selector": {
      "app": "applariat-http-backend"
    },
    "sessionAffinity": "None"
  }
}
