{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "labels": {
      "task": "monitoring",
      "kubernetes.io/name": "monitoring-influxdb"
    },
    "name": "monitoring-influxdb",
    "namespace": "kube-system"
  },
  "spec": {
    "ports": [
      {
        "name": "api",
        "port": 8086,
        "targetPort": 8086
      },
      {
        "name": "client",
        "port": 8083,
        "targetPort": 8083
      }
    ],
    "selector": {
      "k8s-app": "influxdb"
    },
    "sessionAffinity": "None"
  }
}
